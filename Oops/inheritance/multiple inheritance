#multiple inheritance not possible in ruby ,but with possible of mixins

module A   
    def a1
        puts "we in module a1"
    end 
def a2 
    puts "we in module a2"
end 
end 

module B 
    def b1 
        puts "we in module b1"
    end 
    def b2 
        puts "we in module b2"
    end 
end

class Sam  
    include  A 
    include B 

end
a=Sam.new 
a.a1 
a.a2 
a.b1
a.b2
